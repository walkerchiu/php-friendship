{
    "name": "walkerchiu/php-friendship",
    "type": "library",
    "authors": [
        {
            "name":  "Walker Chiu",
            "email": "chenjen.chiou@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "walkerchiu/php-core": "^1.0"
    },
    "suggest": {
        "walkerchiu/php-morph-category": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WalkerChiu\\Friendship\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WalkerChiu\\Friendship\\FriendshipServiceProvider"
            ],
            "aliases": {
                "Friendship": "WalkerChiu\\Friendship\\Models\\Entities\\Friendship"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
